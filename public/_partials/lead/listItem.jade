//- listItem.jade

li(data-category="#{ item.tab }").lead-list-item
  a(href="##{ item.slug }")
    figure(style="background-image:url(#{ item.img })")
    div
      span.name= item.name
      span.score
        span= item.value
        span %
      br
      span.title= item.title

